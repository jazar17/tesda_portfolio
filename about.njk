<section class="main-content__section" id="gallery">
    <div class="container">
        <h2 class="sectionTitle gallery__title animate delay500ms" data-animate="showFromBottomToTOp">Gallery</h2>
        <div class="gallery__rows animate delay500ms" data-animate="showFromBottomToTOp">
            <div class="row gallery__row1">
                {% for img in images.row1 %}
                <div class="col m4 gallery__image-container">
                    <img src="{{img}}" class="gallery__image materialboxed" alt="">
                </div>
                {% endfor %}
            </div>
            <div class="row">
                {% for img in images.row2 %}
                <div class="col m4 gallery__image-container">
                    <img src="{{img}}" class="gallery__image materialboxed" alt="">
                </div>
                {% endfor %}
            </div>
        </div>

    </div>
</section>